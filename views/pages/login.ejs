<%- contentFor('scripts') %>
<script src="/js/codeForLogin.js"></script>

<%- contentFor('hero') %>

<section class="hero">
	<div class="hero-content">
		<h2><span>Login</span></h2>
		<div id="login_credentials">
			<span>
				<h1><span>Username:</span></h1>
				<input type="text" id="username" name="name"></input>
				<h1><span>Password:</span></h1>
				<input type="password" id="password" name="password"></input>
				<h2><input type="submit" onclick="loginCheck()" name="Login" class="btn btn-lg btn-primary"></input></h2>
			</span>
		</div>
</div>
</section>

<main>

	<div class="container">
		<div class="jumbotron text-center">
			Login Page
		</div>
	</div>

</main>